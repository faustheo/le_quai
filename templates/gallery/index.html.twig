{% extends 'base.html.twig' %}

{% block gallery %}
    <div class="container">
        <div class="row justify-content-evenly">
            {% for image in lastImages %}
                <div class=" col-sm-12 col-lg-2"> 
                    <img src="{{ asset('uploads/' ~ image.getImage()) }}" class="img-fluid" alt="{{ image.getTitle() }}" />
                </div>
            {% endfor %}
        </div>
    </div>
{% endblock %}
